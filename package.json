{
  "name": "@js-basics/vector",
  "version": "2.0.0-beta.1",
  "description": "A 3D Vector lib including arithmetic operator overloading (+ - * / % **).",
  "type": "module",
  "directories": {
    "build": "build"
  },
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "clean": "rimraf lib",
    "test": "npm run lint && npm run cover",
    "test:prod": "cross-env BABEL_ENV=production npm run test",
    "test:watch": "npm test -- --watch",
    "test:examples": "node examples/",
    "cover": "mocha --recursive test/* --experimental-specifier-resolution=node",
    "lint": "eslint src",
    "lint:report": "eslint src -f json-relative -o ./eslint-report.json .",
    "build": "node ./build.js",
    "setup": "npm run snyk-protect && npm run clean && npm run test && npm run build",
    "semantic-release": "semantic-release",
    "snyk-protect": "snyk protect"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/basics/vector.git"
  },
  "keywords": [
    "vector",
    "operator overloading",
    "math",
    "arithmetic",
    "basic",
    "vec3",
    "vec2",
    "valueOf"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/basics/vector/issues"
  },
  "homepage": "https://github.com/basics/vector#readme",
  "dependencies": {
    "@babel/core": "^7.16.5",
    "snyk": "^1.797.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "browserslist-to-esbuild": "^1.1.1",
    "chai": "^4.3.4",
    "commitlint": "^15.0.0",
    "esbuild": "^0.14.5",
    "eslint": "8.5.0",
    "eslint-formatter-json-relative": "^0.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.3",
    "mocha": "^9.1.3",
    "pinst": "2.1.6",
    "rimraf": "3.0.2",
    "semantic-release": "18.0.1"
  },
  "snyk": true
}
